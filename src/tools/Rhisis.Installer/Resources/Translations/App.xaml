<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- General -->
    <sys:String x:Key="Ok">OK</sys:String>
    <sys:String x:Key="Cancel">Cancel</sys:String>
    
    <!-- Main Window -->
    <sys:String x:Key="Configure">Configure</sys:String>
    <sys:String x:Key="LoginServer">Login Server and ISC</sys:String>
    <sys:String x:Key="ClusterServer">Cluster Server</sys:String>
    <sys:String x:Key="WorldServer">World Server</sys:String>
    <sys:String x:Key="Database">Database</sys:String>
    <sys:String x:Key="StartInstall">Start install</sys:String>

    <!-- Database configuration window -->
    <sys:String x:Key="DbConfigTitle">Database configuration</sys:String>
    <sys:String x:Key="DbConfigInfo">Configure the Rhisis database access.</sys:String>
    <sys:String x:Key="DbConfigHost">Host</sys:String>
    <sys:String x:Key="DbConfigDatabaseName">Database name</sys:String>
    <sys:String x:Key="DbConfigDatabaseUser">Username</sys:String>
    <sys:String x:Key="DbConfigDatabasePass">Password</sys:String>
    <sys:String x:Key="DbConfigDatabaseProvider">Provider</sys:String>
    <sys:String x:Key="DbConfigDatabasePort">Port</sys:String>
    <sys:String x:Key="DbConfigTest">Test the connection</sys:String>

</ResourceDictionary>